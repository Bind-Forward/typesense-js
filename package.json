    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "axios-mock-adapter": "^1.14.1",
    "babel-eslint": "^8.2.1",
    "babel-plugin-istanbul": "^5.2.0",
    "babelify": "^10.0.0",
    "browserify": "^16.1.1",
    "chai": "~4.1.2",
    "chai-as-promised": "^7.1.1",
    "core-js": "^3.6.5",
    "del": "^3.0.0",
    "eslint": "^4.17.0",
